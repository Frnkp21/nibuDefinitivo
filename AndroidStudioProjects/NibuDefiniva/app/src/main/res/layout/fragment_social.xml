        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:context=".social.SocialFragment">
        <ImageView
            android:id="@+id/nibulogo"
            android:layout_width="84dp"
            android:layout_height="30dp"
            android:contentDescription="nibulogo"
            android:src="@drawable/nibulogo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/socialText" />

            <ImageButton
                android:id="@+id/addFriend"
                android:layout_width="84dp"
                android:layout_height="30dp"
                android:contentDescription="addFriend"
                android:src="@drawable/addfriend"
                android:background="transparent"
                app:layout_constraintTop_toTopOf="@id/socialText"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/socialText"
                app:layout_constraintBottom_toBottomOf="@id/socialText" />

            <ImageButton
                android:id="@+id/notificationFriend"
                android:layout_width="84dp"
                android:layout_height="30dp"
                android:contentDescription="notificationFriend"
                android:src="@drawable/notification"
                android:background="transparent"
                app:layout_constraintTop_toTopOf="@id/socialText"
                app:layout_constraintStart_toEndOf="@id/socialText"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="@id/socialText" />

        <TextView
            android:id="@+id/socialText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SOCIAL"
            android:textSize="30sp"
            android:textColor="@color/white"
            app:layout_constraintTop_toBottomOf="@id/nibulogo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/friend1"/>

